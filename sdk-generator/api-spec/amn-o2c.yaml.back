              - DocumentNumber
              - DocumentTypeCode
              - DespatchDate
              - ArrivalDate
              - References
              - BuyerParty
              - Consignee
            properties:
              IssueDate:
                type: string
              DocumentNumber:
                type: string
              DocumentTypeCode:
                type: string
              Measures:
                type: object
                required:
                  - GrossWeight
                properties:
                  GrossWeight:
                    type: object
                    required:
                      - Measure
                      - MeasureUnitCode
                    properties:
                      Measure:
                        type: string
                      MeasureUnitCode:
                        type: string
                  NetWeight:
                    type: object
                    required:
                      - Measure
                      - MeasureUnitCode
                    properties:
                      Measure:
                        type: string
                      MeasureUnitCode:
                        type: string
                  Volume:
                    type: object
                    required:
                      - Measure
                      - MeasureUnitCode
                    properties:
                      Measure:
                        type: string
                      MeasureUnitCode:
                        type: string
              DespatchDate:
                type: string
              ArrivalDate:
                type: string
              ArrivalTime:
                type: string
              DeliveryOption:
                type: array
                items:
                  type: string
              TotalQuantity:
                type: object
                required:
