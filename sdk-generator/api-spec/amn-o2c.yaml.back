          properties:
            PartyID:
              type: string
            AgencyCode:
              type: string
        Consignee:
          type: object
          properties:
            PartyID:
              type: string
            AgencyCode:
              type: string
            Name:
              type: array
              items:
                type: string
            Address:
              type: object
              required:
                - City
                - CountryCode
              properties:
                Street:
                  type: string
                City:
                  type: string
                District:
                  type: string
                PostCode:
                  type: string
                SubCountry:
                  type: string
                CountryCode:
                  type: string
        OrderedArticle:
          type: object
          required:
            - ArticleIdentification
          properties:
            ArticleIdentification:
