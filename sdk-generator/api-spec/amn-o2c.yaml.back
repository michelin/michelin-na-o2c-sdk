        DocumentID:
          type: string
        Variant:
          type: string
        DespatchFromDate:
          type: string
        DespatchToDate:
          type: string
        References:
          type: object
          required:
            - ReferenceTypeCode
            - DocumentID
          properties:
            ReferenceTypeCode:
              type: string
            DocumentID:
              type: string
        SellerParty:
          type: object
          required:
            - PartyID
            - AgencyCode
          properties:
            PartyID:
              type: string
            AgencyCode:
              type: string
        SupplierParty:
          type: object
          required:
            - PartyID
            - AgencyCode
          properties:
            PartyID:
              type: string
            AgencyCode:
              type: string
        BuyerParty:
