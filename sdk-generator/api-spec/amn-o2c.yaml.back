                  properties:
                    ReferenceTypeCode:
                      type: string
                    DocumentID:
                      type: string
                    IssueDate:
                      type: string
              BuyerParty:
                type: object
                required:
                  - PartyID
                  - AgencyCode
                properties:
                  PartyID:
                    type: string
                  AgencyCode:
                    type: string
              SupplierParty:
                type: object
                required:
                  - PartyID
                  - AgencyCode
                properties:
                  PartyID:
                    type: string
                  AgencyCode:
                    type: string
                  SalesContact:
                    type: object
                    required:
                      - Name
                    properties:
                      Name:
                        type: string
                  ShippingLocation:
                    type: object
                    required:
                      - LocationName
                    properties:
                      LocationName:
                        type: string
              OrderingParty:
                type: object
                required:
                  - PartyID
                  - AgencyCode
                properties:
                  PartyID:
                    type: string
                  AgencyCode:
                    type: string
              Consignee:
                type: object
                properties:
                  PartyID: 
                    type: string
                  AgencyCode: 
                    type: string
                  Name: 
                    type: array
                    items:
                      type: string
                  Address:
                    type: object
                    required:
                      - City
                      - CountryCode
                    properties:
                      Street:
                        type: array
                        items:
                          type: string
                      City:
                        type: string
                      District:
                        type: string
                      PostCode:
                        type: string
                      PostBoxCode:
                        type: string
                      SubCountry:
                        type: string
                      CountryCode:
                        type: string
                      Geolocation:
                        type: object
                        required:
                          - Latitude
                          - Longitude
                        properties:
                          Latitude:
                            type: string
                          Longitude:
                            type: string
              SellerParty:
                type: object
                required:
                  - PartyID
                  - AgencyCode
                properties:
                  PartyID:
                    type: string
                  AgencyCode:
                    type: string
              CarrierParty:
                type: object
                required:
                  - PartyID
                  - AgencyCode
                properties:
                  PartyID:
                    type: string
                  AgencyCode:
                    type: string
              LineLevel:
                type: array
                items:
                  type: object
                  required:
                    - LineID
                    - OrderedArticle
                  properties:
                    LineID:
                      type: string
                    OrderingMethod:
                      type: string
                    References:
                      type: array
                      items:
                        type: object
                        required:
                          - ReferenceTypeCode
                          - DocumentID
                          - LineID
                        properties:
                          ReferenceTypeCode:
                            type: string
                          DocumentID:
                            type: string
                          LineID:
                            type: string
                          IssueDate:
                            type: string
                    OrderedArticle:
                      type: object
                      required:
                        - ArticleIdentification
                        - ArticleDescription
                        - DespatchedQuantity
                      properties:
                        ArticleIdentification:
                          type: object
                          properties:
                            BuyersArticleID:
                              type: string
                            ManufacturersArticleID:
                              type: string
                            EANUCCArticleID:
                              type: string
                        ArticleDescription:
                          type: object
                          required:
                            - ArticleDescriptionText
                          properties:
                            ArticleDescriptionText:
                              type: array
                              items:
                                type: string
                        DespatchedQuantity:
                          type: object
                          required:
                            - QuantityValue
                            - MeasureUnitCode
                          properties:
                            QuantityValue:
                              type: string
                            MeasureUnitCode:
                              type: string
                        DeliveryOption:
                          type: array
                          items:
                            type: string
                        TracingInformation:
                          type: array
                          items:
                            type: object
                            properties:
                              TracingID:
                                type: string
                              SerialNumber:
                                type: string
                              TracingURI:
